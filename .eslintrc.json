{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "react-app",
        "react-app/jest",
        "eslint:recommended",
        "plugin:react/recommended",
        "prettier"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": 2020,    
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/react-in-jsx-scope": "off",
        "semi" : "warn",
        "quotes" : "warn",
        "block-spacing": "warn",
        "no-whitespace-before-property" :"warn",
        "react/prop-types": "off"
    }
}
